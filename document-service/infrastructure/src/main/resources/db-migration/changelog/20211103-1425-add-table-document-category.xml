<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
   http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="20211103-1425-001" author="DucAnh">
        <createTable tableName="document-category">
            <column name="document_id" type="integer">
                <constraints primaryKey="true" primaryKeyName="pk_document_id"/>
            </column>
            <column name="category_id" type="integer">
                <constraints primaryKey="true" primaryKeyName="pk_category_id"/>
            </column>
            <column name="created_date" type="TIMESTAMP"/>
            <column name="last_updated_date" type="TIMESTAMP"/>
        </createTable>
    </changeSet>
    <changeSet id="20211103-1425-001" author="DucAnh">
        <addForeignKeyConstraint
                baseTableName="document_category" baseColumnNames="document_id"
                constraintName="fk_document_category_relation_document_by_documnet_id"
                referencedTableName="document" referencedColumnNames="id"/>
        <addForeignKeyConstraint
                baseTableName="document_category" baseColumnNames="category_id"
                constraintName="fk_documnet_category_relation_category_by_category_id"
                referencedTableName="category" referencedColumnNames="id"/>
    </changeSet>


</databaseChangeLog>