<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
   http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="20211103-1425-001" author="DucAnh">
        <createTable tableName="document">
            <column name="document_id" type="integer" autoIncrement="true">
                <constraints primaryKey="true" primaryKeyName="pk_document_id"/>
            </column>
            <column name="author" type="varchar(32)"/>
            <column name="description" type="text"/>
            <column name="file_s3_object_key" type="varchar(256)"/>
            <column name="thumb_s3_object_key" type="varchar(256)"/>
            <column name="title" type="varchar(128)"/>
            <column name="type" type="varchar(16)"/>
            <column name="created_by" type="integer"/>
            <column name="last_updated_date" type="integer"/>
            <column name="created_date" type="TIMESTAMP"/>
            <column name="last_updated_date" type="TIMESTAMP"/>
        </createTable>
    </changeSet>



</databaseChangeLog>