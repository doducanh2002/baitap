<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="20213007-1628-001" author="ducanh">
        <createTable tableName="poduct">
            <column name="id" type="INTEGER" autoIncrement="true">
                <constraints primaryKey="true" primaryKeyName="pk_product_id"/>
            </column>
            <column name="name" type="VARCHAR(256)"/>
            <column name="price" type="INTEGER"/>
            <column name="expired" type="DATE"/>
        </createTable>


        <createTable tableName="review">
            <column name="id" type="INTEGER" autoIncrement="true">
                <constraints primaryKey="true" primaryKeyName="pk_product_id"/>
            </column>
            <column name="name reviewer" type="VARCHAR(256)"/>
            <column name="product" type="VARCHAR(256)"/>
            <column name="review product" type="VARCHAR(256)"/>
            <column name="date" type="DATE"/>
        </createTable>
    </changeSet>

</databaseChangeLog>